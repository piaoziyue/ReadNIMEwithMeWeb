# Gemfile for a Jekyll project

source "https://rubygems.org"

# Specify the version of Jekyll
gem "jekyll", "~> 4.2.0"

# Include additional plugins as needed
gem "jekyll-feed"
gem "jekyll-seo-tag"

# Uncomment the following line if you're using GitHub Pages
# gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
end

gem 'webrick'

